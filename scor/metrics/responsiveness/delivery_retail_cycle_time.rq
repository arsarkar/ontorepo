PREFIX    :   <http://purl.org/eis/vocab/scor#> 
PREFIX xsd:   <http://www.w3.org/2001/XMLSchema#> 

SELECT (xsd:double((?value1
				   +?value2
				   +?value3
				   +?value4
				   +?value5
				   +?value6)) AS ?metricResult)
WHERE 
{
	?order :hasMetricRS_17  ?value1 .
	?order :hasMetricRS_32  ?value2 .
	?order :hasMetricRS_34  ?value3 .
	?order :hasMetricRS_97  ?value4 .
	?order :hasMetricRS_109  ?value5 .
	?order :hasMetricRS_129  ?value6 .
}
