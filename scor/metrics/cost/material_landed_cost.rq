PREFIX    :   <http://purl.org/eis/vocab/scor#> 
PREFIX xsd:   <http://www.w3.org/2001/XMLSchema#> 

SELECT (xsd:double((?value1
				   +?value2
				   +?value3
				   +?value4)) AS ?metricResult)
WHERE 
{
	?order :hasMetricCO_9   ?value1 .
	?order :hasMetricCO_10  ?value2 .
	?order :hasMetricCO_11  ?value3 .
	?order :hasMetricCO_12  ?value4 .
}
