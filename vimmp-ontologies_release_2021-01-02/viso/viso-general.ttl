##############  (5) solver feature ###############################

viso:solver_feature a owl:Class;
  rdfs:comment "Capability of a software tool, intended as a numerical aspect or an algorithm which is implemented. ('solver' intended as in MODA)"^^xs:string;
  rdfs:comment "An aspect of the numerical method used to solve the governing equations."^^xs:string;
  rdfs:subClassOf evmpo:marketplace_related_entity.  

viso:el_solver_feature a owl:Class;
  rdfs:subClassOf viso:solver_feature;
  owl:intersectionOf (viso:solver_feature viso:R_IS_ELECTRONIC).

viso:am_solver_feature a owl:Class;
  rdfs:subClassOf viso:solver_feature;
  owl:intersectionOf (viso:solver_feature [a owl:Class; owl:unionOf (viso:R_IS_ATOMISTIC viso:R_IS_MESOSCOPIC)]).

viso:co_solver_feature a owl:Class;
  rdfs:subClassOf viso:solver_feature;
  owl:intersectionOf (viso:solver_feature viso:R_IS_CONTINUUM).

viso:execution_mode a owl:Class;
  rdfs:subClassOf viso:solver_feature;
  owl:oneOf (viso:SERIAL_EXECUTION viso:PARALLEL_EXECUTION).

##############  (6) model feature ################################

viso:model_feature a owl:Class; 
  rdfs:comment "Capability of a software tool, intended as a model aspect that can be addressed. ('model' intended as in MODA)."^^xs:string;
  rdfs:subClassOf evmpo:marketplace_related_entity.

viso:el_model_feature a owl:Class;
  rdfs:subClassOf viso:model_feature;
  owl:intersectionOf (viso:model_feature viso:R_IS_ELECTRONIC).

viso:el_materials_relation_trait a owl:Class;
  rdfs:subClassOf viso:el_model_feature.

viso:el_physical_equation_trait a owl:Class;
  rdfs:subClassOf viso:el_model_feature.

viso:el_external_condition_trait a owl:Class;
  rdfs:subClassOf viso:el_model_feature.

viso:el_materials_relation a owl:Class;
  rdfs:subClassOf viso:el_materials_relation_trait.

viso:el_physical_equation a owl:Class;
  rdfs:subClassOf viso:el_physical_equation_trait.

viso:am_model_feature a owl:Class;
  rdfs:subClassOf viso:model_feature;
  owl:intersectionOf (viso:model_feature [a owl:Class; owl:unionOf (viso:R_IS_ATOMISTIC viso:R_IS_MESOSCOPIC)]).

viso:am_materials_relation_trait a owl:Class;
  rdfs:subClassOf viso:am_model_feature.

viso:am_physical_equation_trait a owl:Class;
  rdfs:subClassOf viso:am_model_feature.

viso:am_external_condition_trait a owl:Class;
  rdfs:subClassOf viso:am_model_feature.

viso:am_materials_relation a owl:Class;
  rdfs:subClassOf viso:am_materials_relation_trait.

viso:am_physical_equation a owl:Class;
  rdfs:subClassOf viso:am_physical_equation_trait.

viso:co_model_feature a owl:Class;
  rdfs:subClassOf viso:model_feature;
  owl:intersectionOf (viso:model_feature viso:R_IS_CONTINUUM).

viso:co_materials_relation_trait a owl:Class;
  rdfs:subClassOf viso:co_model_feature.

viso:co_physical_equation_trait a owl:Class;
  rdfs:subClassOf viso:co_model_feature.

viso:co_external_condition_trait a owl:Class;
  rdfs:subClassOf viso:co_model_feature.

viso:co_materials_relation a owl:Class;
  rdfs:subClassOf viso:co_materials_relation_trait.

viso:co_physical_equation a owl:Class;
  rdfs:subClassOf viso:co_physical_equation_trait.

viso:physical_equation a owl:Class;
  rdfs:subClassOf osmo:governing_equation;
  owl:equivalentClass osmo:physical_equation;
  owl:disjointUnionOf (viso:el_physical_equation viso:am_physical_equation viso:co_physical_equation).

viso:materials_relation a owl:Class;
  rdfs:subClassOf osmo:governing_equation;
  owl:equivalentClass osmo:materials_relation;
  owl:disjointUnionOf (viso:el_materials_relation viso:am_materials_relation viso:co_materials_relation).

viso:materials_relation_trait a owl:Class;
  rdfs:subClassOf evmpo:marketplace_related_entity;
  owl:disjointUnionOf (viso:el_materials_relation_trait viso:am_materials_relation_trait viso:co_materials_relation_trait).

viso:physical_equation_trait a owl:Class;
  rdfs:subClassOf viso:model_feature;
  owl:disjointUnionOf (viso:el_physical_equation_trait viso:am_physical_equation_trait viso:co_physical_equation_trait).

viso:external_condition_trait a owl:Class;
  rdfs:subClassOf viso:model_feature;
  owl:disjointUnionOf (viso:el_external_condition_trait viso:am_external_condition_trait viso:co_external_condition_trait).

viso:model_object a owl:Class;
  rdfs:subClassOf viso:materials_relation_trait;
  rdfs:comment "Type of object entering the model and carrying the degrees of freedom."^^xs:string;
  rdfs:comment "E.g., composite or structureless, rigid or flexible, classical or quantum objects."^^xs:string.

##############  (7) modeling related entity ######################

viso:modeling_related_entity a owl:Class;
  rdfs:comment"High level concept related to modelling, as statistical mechanics, the RoMM model types, fundamental physics equation, etc."^^xs:string;
  rdfs:subClassOf evmpo:marketplace_related_entity.  

viso:model_type a owl:Class;
  rdfs:subClassOf viso:modeling_related_entity.

viso:am_model_type a owl:Class;
  rdfs:subClassOf viso:model_type;
  owl:intersectionOf (viso:model_type [a owl:Class; owl:unionOf (viso:R_IS_ATOMISTIC viso:R_IS_MESOSCOPIC)]).

viso:el_model_type a owl:Class;
  rdfs:subClassOf viso:model_type;
  owl:intersectionOf (viso:model_type viso:R_IS_ELECTRONIC).

viso:co_model_type a owl:Class;
  rdfs:subClassOf viso:model_type;
  owl:intersectionOf (viso:model_type viso:R_IS_CONTINUUM).

viso:model_type owl:disjointUnionOf (viso:am_model_type viso:el_model_type viso:co_model_type).

##############  (8) Software update ##############################

viso:software_update a owl:Class;
   rdfs:subClassOf evmpo:process;
   rdfs:comment "To describe (as text) the changes between versions of a software."^^xs:string;
   rdfs:subClassOf [a owl:Restriction;
  		   owl:onProperty viso:has_input;
  		   owl:qualifiedCardinality 1;
   		   owl:onClass viso:software];
   rdfs:subClassOf [a owl:Restriction;
  		   owl:onProperty viso:has_output;
  		   owl:qualifiedCardinality 1;
   		   owl:onClass viso:software];
   rdfs:subClassOf [a owl:Restriction;
  		   owl:onProperty viso:has_update_comment;
 		   owl:allValuesFrom xs:string].

viso:software_tool_update a owl:Class; 
  rdfs:subClassOf viso:software_update;
  rdfs:comment "Allows to describe the addition/removal of features between versions of a software tool."^^xs:string;
  rdfs:subClassOf [a owl:Restriction;
  		   owl:onProperty viso:has_input;
  		   owl:qualifiedCardinality 1;
   		   owl:onClass viso:software_tool];
   rdfs:subClassOf [a owl:Restriction;
  		   owl:onProperty viso:has_output;
  		   owl:qualifiedCardinality 1;
   		   owl:onClass viso:software_tool];
   rdfs:subClassOf [a owl:Restriction;
   		   owl:onProperty viso:adds_feature;
 		   owl:allValuesFrom viso:software_tool_feature];
   rdfs:subClassOf [a owl:Restriction;
  		   owl:onProperty viso:removes_feature;
 		   owl:allValuesFrom viso:software_tool_feature];
   rdfs:subClassOf [a owl:Restriction;
  		   owl:onProperty viso:has_update_comment;
 		   owl:allValuesFrom xs:string].

##############  (9) Software interface ###########################

viso:software_interface a owl:Class;
  rdfs:subClassOf evmpo:software_infrastructure; 
  rdfs:comment "Interface between a software and a user or a client (i.e., a program or device)"^^xs:string;
  rdfs:comment "Some sub-classes of this class are taken from the SWO software interface class (swo:SWO_9000050)"^^xs:string.

viso:graphical_user_interface a owl:Class;
  rdfs:comment "graphical user interface (GUI)"^^xs:string;
  rdfs:subClassOf viso:software_interface;
  owl:equivalentClass swo:SWO_9000052. # <-> "swo graphical user interface"
swo:SWO_9000052 rdfs:subClassOf viso:software_interface.

viso:command_line_interface a owl:Class;
  rdfs:comment "command line interface (CLI)"^^xs:string;
  rdfs:subClassOf viso:software_interface;
  owl:disjointWith viso:graphical_user_interface;
  owl:equivalentClass swo:SWO_9000053. # <-> "swo command line interface"
swo:SWO_9000053 rdfs:subClassOf viso:software_interface.

viso:application_programming_interface a owl:Class;
  rdfs:comment "application programming interface (API)"^^xs:string;
  rdfs:subClassOf viso:software_interface;
  owl:equivalentClass swo:SWO_9000054. # <-> "swo application programming interface"
swo:SWO_9000054 rdfs:subClassOf viso:software_interface.

viso:wrapper a owl:Class;
  rdfs:subClassOf viso:software_interface;
  rdfs:comment """wrapper (interface): an outer (more abstract) 
  layer as compared to an application programming interface, 
  and usually providing a reduced and more specific set of 
  functionalities"""^^xs:string.

viso:data_interface a owl:Class; 
  rdfs:comment "interface for data, can be file-based or stream-based"^^xs:string;
  rdfs:subClassOf viso:software_interface.

viso:file_based_io a owl:Class; 
  rdfs:subClassOf viso:data_interface.

viso:stream_based_io a owl:Class; 
  rdfs:subClassOf viso:data_interface.

# definition of individuals for software interface

viso:TEXT_FILE_IO a viso:file_based_io.
viso:BINARY_FILE_IO a viso:file_based_io.

##############  (10) Classifier ###################################

viso:classifier a owl:Class; 
  rdfs:comment "A classifier (e.g., the granularity level) that is relevant for materials modelling software "^^xs:string;
  rdfs:subClassOf evmpo:annotation.  

viso:granularity_level rdfs:subClassOf viso:classifier;
  owl:equivalentClass osmo:granularity_level;
  owl:oneOf (osmo:ELECTRONIC osmo:ATOMISTIC osmo:MESOSCOPIC osmo:CONTINUUM).
# NB: For convenience, I duplicate here the definitions of granularity given in OSMO
# Can be removed if OSMO is loaded

##################################################################

### Datatype Properties ###

####### VISO primitive (or fundamental) properties ###############

# P1 - DP-Parthood #

viso:parthood_dp_relation a owl:DatatypeProperty;
  rdfs:subPropertyOf viprs:immanent_datatype_property;
  rdfs:comment """Datatype property based on objects constitution (also in the sense of participants in a process).
  	       It can be in both directions (i.e., having parts or being parts of something)."""^^xs:string.

# P2 - DP-Dependence #

viso:dependence_dp_relation a owl:DatatypeProperty;
  rdfs:subPropertyOf viprs:immanent_datatype_property;
  rdfs:comment """Datatype property based on any kind of dependence (e.g, functional dependence,	
  	       legal dependence, causality, authorship). It can be in both directions 
	       (i.e., depending on something or having dependents). """^^xs:string.

# P3 - DP-Ability #

viso:ability_dp_relation a owl:DatatypeProperty;
  rdfs:subPropertyOf viprs:immanent_datatype_property;
  rdfs:comment """Datatype property based on some functionality, capability, i.e., the potentiality to do something.
  	       It concerns the functioning of an object. It can be in both directions 
	       (i.e., having an ability or being an ability)."""^^xs:string.

# P4 - DP-Documentation #

viso:documentation_dp_relation a owl:DatatypeProperty;
  rdfs:subPropertyOf viprs:immanent_datatype_property;
  rdfs:comment """Datatype property based on documentation (e.g, descriptions, explanations or classifications).
  	       It can be in both directions (i.e., having some documentation or being documentation of something)."""^^xs:string.

# P5 - DP-Similarity #

viso:similarity_dp_relation a owl:DatatypeProperty;
  rdfs:subPropertyOf viprs:immanent_datatype_property;
  rdfs:comment "Datatype property based on similarity, intended as some common aspect."^^xs:string.

# P6 - Connection #

viso:connection_dp_relation a owl:DatatypeProperty;
  rdfs:subPropertyOf viprs:immanent_datatype_property;
  rdfs:comment "Datatype property that is other than parthood, ability, dependence, similarity and documentation."^^xs:string.

##################################################################

viso:has_main_name a owl:DatatypeProperty;
  a owl:FunctionalProperty;
  rdfs:comment "identifies the main name of a software, without version information"^^xs:string;
  rdfs:domain viso:software;
  rdfs:range xs:string;
  rdfs:subPropertyOf viso:documentation_dp_relation;
  rdfs:subPropertyOf viprs:has_name.

viso:has_version_identifier a owl:DatatypeProperty;
  a owl:FunctionalProperty;
  rdfs:comment "identifies the version of a software (can be a number or a suffix in the name)"^^xs:string;
  rdfs:domain viso:software;
  rdfs:range xs:string;
  rdfs:subPropertyOf viso:documentation_dp_relation;
  rdfs:subPropertyOf viprs:has_specifier.

viso:has_release_date a owl:DatatypeProperty;
  a owl:FunctionalProperty;
  rdfs:domain viso:software;
#  rdfs:range xs:date; # NB: This would be the appropriate datatype, but it is currently not recognized by FaCT++
  rdfs:range xs:dateTime; 
  rdfs:comment """Note: xs:date would be the appropriate datatype, we don't use it because it is currently not recognized by the FaCT++ reasoner.
  	       Therefore, conventionally, we set the time as T00:00:00+00:00 for all individuals."""^^xs:string;
  rdfs:subPropertyOf viso:documentation_dp_relation;
  rdfs:subPropertyOf viprs:has_specifier.
  
viso:is_free a owl:DatatypeProperty, owl:FunctionalProperty;
  rdfs:domain viso:software;
  rdfs:range xs:boolean;
  rdfs:subPropertyOf viso:dependence_dp_relation;
  rdfs:subPropertyOf viprs:has_flag.

viso:is_free_to_academic a owl:DatatypeProperty, owl:FunctionalProperty;
  rdfs:domain viso:software;
  rdfs:range xs:boolean;
  rdfs:subPropertyOf viso:dependence_dp_relation;
  rdfs:subPropertyOf viprs:has_flag.

viso:has_a_gpu_version a owl:DatatypeProperty, owl:FunctionalProperty;
  rdfs:domain viso:software;
  rdfs:range xs:boolean;
  rdfs:subPropertyOf viso:ability_dp_relation;
  rdfs:subPropertyOf viprs:has_flag.

viso:is_a_library a owl:DatatypeProperty, owl:FunctionalProperty;
  rdfs:domain viso:software_tool;
  rdfs:range xs:boolean;
  rdfs:subPropertyOf viso:ability_dp_relation;
  rdfs:subPropertyOf viprs:has_flag.

viso:is_open_source a owl:DatatypeProperty, owl:FunctionalProperty;
  rdfs:seeAlso "https://opensource.org/osd"^^xs:string;
  rdfs:comment """Open source means there is access to the source code, 
  but also that the distribution terms comply with certain criteria 
  (e.g., about re-distribution)"""^^xs:string;
  rdfs:domain viso:software;
  rdfs:range xs:boolean;
  rdfs:subPropertyOf viso:dependence_dp_relation;
  rdfs:subPropertyOf viso:ability_dp_relation;
  rdfs:subPropertyOf viprs:has_flag.

viso:has_a_gui a owl:DatatypeProperty, owl:FunctionalProperty;
  rdfs:domain viso:software;
  rdfs:range xs:boolean;
  rdfs:subPropertyOf viso:ability_dp_relation;
  rdfs:subPropertyOf viprs:has_flag.

viso:has_a_manual a owl:DatatypeProperty, owl:FunctionalProperty;
  rdfs:domain viso:software;
  rdfs:range xs:boolean;
  rdfs:subPropertyOf viso:documentation_dp_relation;
  rdfs:subPropertyOf viprs:has_flag.

viso:has_memory_requirement a owl:DatatypeProperty; 
  rdfs:domain viso:software;
  rdfs:range xs:string;
  rdfs:comment "Generic memory requirement, can concern both storage and RAM"^^xs:string;
  rdfs:subPropertyOf viso:dependence_dp_relation;
  rdfs:subPropertyOf viprs:has_specifier.

viso:has_model_classifier a owl:DatatypeProperty;
  rdfs:domain viso:model_type;  
  rdfs:range xs:string;
  rdfs:subPropertyOf viso:documentation_dp_relation;
  rdfs:subPropertyOf viprs:has_specifier.

viso:has_osmo_pe_type_id a owl:DatatypeProperty;
  rdfs:subPropertyOf viso:has_model_classifier;
  rdfs:domain viso:model_type;  
  rdfs:range xs:string.

viso:has_update_comment a owl:DatatypeProperty;
  rdfs:domain viso:software_update;
  rdfs:range xs:string;
  rdfs:subPropertyOf viso:parthood_dp_relation;
  rdfs:subPropertyOf viso:documentation_dp_relation;
  rdfs:subPropertyOf viprs:has_textual_content.

viso:has_a_cli a owl:DatatypeProperty, owl:FunctionalProperty;
  rdfs:domain viso:software;
  rdfs:range xs:boolean;
  rdfs:subPropertyOf viso:ability_dp_relation;
  rdfs:subPropertyOf viprs:has_flag.

viso:has_an_api a owl:DatatypeProperty, owl:FunctionalProperty;
  rdfs:domain viso:software;
  rdfs:range xs:boolean;
  rdfs:subPropertyOf viso:ability_dp_relation;
  rdfs:subPropertyOf viprs:has_flag.

viso:has_a_wrapper a owl:DatatypeProperty, owl:FunctionalProperty;
  rdfs:domain viso:software;
  rdfs:range xs:boolean;
  rdfs:subPropertyOf viso:ability_dp_relation;
  rdfs:subPropertyOf viprs:has_flag.

viso:has_a_data_interface a owl:DatatypeProperty, owl:FunctionalProperty;
  rdfs:domain viso:software;
  rdfs:range xs:boolean;
  rdfs:subPropertyOf viso:ability_dp_relation;
  rdfs:subPropertyOf viprs:has_flag.

viso:has_accessible_exe a owl:DatatypeProperty, owl:FunctionalProperty;
  rdfs:comment """the tool executable can be accessed. It implies there is a command line interface in the 
  sense that the executable can be called from a terminal"""^^xs:string; 
  rdfs:domain viso:software_tool;
  rdfs:range xs:boolean;
  rdfs:subPropertyOf viprs:has_flag.

### Object Properties ###

####### VISO primitive (or fundamental) properties ###############

# P1 - Parthood #

viso:parthood_relation a owl:ObjectProperty;
  rdfs:subPropertyOf viprs:immanent_relation;  # overlap
  rdfs:comment """Relates objects based on the their constitution (also in the sense of participants in a process).
  	       It can be in both directions (i.e., having parts or being parts of something)."""^^xs:string.

viso:has_part a owl:ObjectProperty; 
  rdfs:comment "It concerns the constitution of an object (also in the sense of the participants of a process)."^^xs:string;
  rdfs:subPropertyOf viso:parthood_relation, viprs:mereosemiotics_hp_ip.
viprs:has_proper_part rdfs:subPropertyOf viso:has_part.

viso:is_part_of a owl:ObjectProperty; 
  owl:inverseOf viso:has_part;
  rdfs:comment "It concerns the constitution of an object (also in the sense of the participants of a process)."^^xs:string;
  rdfs:subPropertyOf viso:parthood_relation, viprs:mereosemiotics_hp_ip.
viprs:is_proper_part_of rdfs:subPropertyOf viso:is_part_of.

# P2 - Dependence #

viso:dependence_relation a owl:ObjectProperty;
  rdfs:subPropertyOf viprs:immanent_relation;
  rdfs:comment """Relates objects with any kind of dependence (e.g, functional dependence,	
  	       legal dependence, causality, authorship). It can be in both directions 
	       (i.e., depending on something or having dependents). """^^xs:string.

viso:depends_on a owl:ObjectProperty;
  rdfs:comment """Refers to any kind of dependence. For example, functional dependence (e.g., a tool requires a library),	
  	       legal dependence (e.g., a product is owned/distribued by an agent), 
	       causality (e.g., something exists or happens after something else), authorship (e.g, a book has an author). """^^xs:string.

viso:has_dependent a owl:ObjectProperty;
  owl:inverseOf viso:depends_on;
  rdfs:comment """Refers to any kind of dependence. For example, functional dependence (e.g., a library is required by a tool), 
  	       legal dependence (e.g., an agent owns/distributes a product), 
	       causality  (e.g., something exists or happens beofre something else), authorship (e.g, an author writes a book). """^^xs:string.

# P3 - Ability #

viso:ability_relation a owl:ObjectProperty;
  rdfs:subPropertyOf viprs:immanent_relation;
  rdfs:comment """Relates objects based on some functionality, capability, i.e., the potentiality to do something.
  	       It concerns the functioning of an object. It can be in both directions (having an ability or being an ability)."""^^xs:string.

viso:has_ability a owl:ObjectProperty;
  rdfs:subPropertyOf viprs:enables_entity_containing.

viso:is_ability_of a owl:ObjectProperty;
  owl:inverseOf viso:has_ability;
  rdfs:subPropertyOf viprs:is_proper_part_of_entity_enabled_by.

# P4 - Documentation #

viso:documentation_relation a owl:ObjectProperty;
  rdfs:subPropertyOf viprs:immanent_relation;
  rdfs:comment """Relates objects based on documentation (e.g, descriptions, explanations or classifications).
  	       It can be in both directions (i.e., having some documentation or being documentation of something)."""^^xs:string.

viso:has_documentation a owl:ObjectProperty;
  rdfs:comment "Points to a description, explanation or classification of an object."^^xs:string;
  rdfs:subPropertyOf viprs:has_sign.

viso:is_documentation_of a owl:ObjectProperty;
  owl:inverseOf viso:has_documentation;
  rdfs:comment "Is a description, explanation or classification of an object."^^xs:string;
  rdfs:subPropertyOf viprs:is_sign_for.

# P5 - Similarity #

viso:similarity_relation a owl:ObjectProperty;
  rdfs:subPropertyOf viprs:immanent_relation;
  rdfs:comment "Relates objects based on similarity, intended as some common aspect."^^xs:string.

viso:is_similar_to a owl:ObjectProperty;
#  a owl:ReflexiveProperty; # NB: Removed to avoid uninformative relations between individuals.
  a owl:TransitiveProperty;
  a owl:SymmetricProperty;
  rdfs:comment """Refers to any kind of similarity, intended as some common aspect.
  	       It is a transitive and symmetric property. We intentionally avoid reflexivity, since it would be uninformative."""^^xs:string.

# P6 - Connection #

viso:connection_relation a owl:ObjectProperty;
  rdfs:subPropertyOf viprs:immanent_relation;
  rdfs:comment "Relates objects in ways that are other than parthood, ability, dependence, similarity and documentation."^^xs:string.

viso:is_connected_to a owl:ObjectProperty.

##################################################################

viso:depends_on rdfs:subPropertyOf viso:dependence_relation.
viso:has_dependent rdfs:subPropertyOf viso:dependence_relation.

viso:has_ability rdfs:subPropertyOf viso:ability_relation.
viso:is_ability_of rdfs:subPropertyOf viso:ability_relation.

viso:has_documentation rdfs:subPropertyOf viso:documentation_relation.
viso:is_documentation_of rdfs:subPropertyOf viso:documentation_relation.

viso:has_part rdfs:subPropertyOf viso:parthood_relation.
viso:is_part_of rdfs:subPropertyOf viso:parthood_relation.

viso:is_similar_to rdfs:subPropertyOf viso:similarity_relation.

viso:is_connected_to rdfs:subPropertyOf viso:connection_relation.

##################################################################

viso:is_owner_of a owl:ObjectProperty;
  rdfs:domain viso:agent;
  rdfs:range viso:software;
  rdfs:subPropertyOf viso:has_dependent;
  rdfs:subPropertyOf viprs:n_loc_cor_rnoc.  

viso:has_owner a owl:ObjectProperty;
  rdfs:domain viso:software;
  rdfs:range viso:agent;
  owl:inverseOf viso:is_owner_of;
  rdfs:subPropertyOf viso:depends_on;
  rdfs:subPropertyOf viprs:n_lnoc_cor_roc.

viso:is_distributor_of a owl:ObjectProperty;
  rdfs:domain viso:agent;
  rdfs:range viso:software;
  owl:inverseOf viso:is_distributed_by;
  rdfs:subPropertyOf viso:has_dependent;
  rdfs:subPropertyOf viprs:n_loc_cor_rnoc.  

viso:is_distributed_by a owl:ObjectProperty;
  rdfs:domain viso:software;
  rdfs:range viso:agent;
  rdfs:subPropertyOf viso:depends_on;
  rdfs:subPropertyOf viprs:n_lnoc_cor_roc.

viso:has_license a owl:ObjectProperty;
  rdfs:domain viso:software;
  rdfs:range viso:license;
  rdfs:subPropertyOf swo:has_license;
  rdfs:subPropertyOf viso:depends_on;
  rdfs:subPropertyOf viprs:n_lnoc_cor_roc.
swo:has_license rdfs:subPropertyOf viprs:n_lnoc_cor_roc.

viso:is_input_of a owl:ObjectProperty;
  rdfs:domain viso:software;
  rdfs:range viso:software_update;
  owl:inverseOf viso:has_input;
  rdfs:subPropertyOf viso:is_part_of.

viso:is_output_of a owl:ObjectProperty;
  rdfs:domain viso:software;
  rdfs:range viso:software_update;
  owl:inverseOf viso:has_output;
  rdfs:subPropertyOf viso:is_part_of.

viso:has_vimmp_contact a owl:ObjectProperty;  # NB: "internal" VIMMP property
  rdfs:comment "points to the VIMMP human agent(s) to be contacted about the given software"^^xs:string;
  rdfs:domain viso:software;
  rdfs:range evmpo:human_agent;
  rdfs:subPropertyOf viso:is_connected_to;
  rdfs:subPropertyOf viprs:immanent_relation.

viso:has_manual a owl:ObjectProperty;
  rdfs:domain viso:software;
  rdfs:range evmpo:training_document;
  rdfs:subPropertyOf viso:has_documentation.

viso:is_manual_of a owl:ObjectProperty; 
  rdfs:domain evmpo:training_document;
  rdfs:range viso:software;
  owl:inverseOf viso:has_manual;
  rdfs:subPropertyOf viso:is_documentation_of.

viso:is_tool_for_model a owl:ObjectProperty;
  rdfs:comment "Relates software tools and RoMM models."^^xs:string;
  rdfs:domain viso:software_tool;
  rdfs:range viso:model_type;
  rdfs:subPropertyOf viso:has_ability.

viso:has_feature a owl:ObjectProperty;
  rdfs:comment "To describe the (solver and model) features of a software tool."^^xs:string;
  rdfs:domain viso:software_tool;
  rdfs:range viso:software_tool_feature;
  rdfs:subPropertyOf viso:has_ability.

viso:is_feature_of a owl:ObjectProperty;
  rdfs:domain viso:software_tool_feature;
  rdfs:range viso:software_tool;
  owl:inverseOf viso:has_feature;
  rdfs:subPropertyOf viso:is_ability_of.

viso:uses_language a owl:ObjectProperty;
  rdfs:domain [a owl:Class; owl:unionOf (viso:software_tool viso:software_interface)];
  rdfs:range viso:programming_language;
  rdfs:subPropertyOf viso:depends_on;
  rdfs:subPropertyOf viprs:n_lnoc_or_roc.

viso:is_compatible_with a owl:ObjectProperty,
  owl:SymmetricProperty;
  rdfs:comment "Asserts compatibility (i.e., the ability to exchange information directly, with no need to interface) between sofware tools."^^xs:string;
  rdfs:domain viso:software_tool;
  rdfs:range viso:software_tool;
  rdfs:subPropertyOf viso:is_connected_to;
  rdfs:subPropertyOf viprs:can_cooccur_with.

viso:requires a owl:ObjectProperty;
  rdfs:comment "Relates a software tool to libraries and/or operating systems."^^xs:string;
  rdfs:domain viso:software_tool;
  rdfs:range viso:software;
  rdfs:subPropertyOf viso:depends_on;
  rdfs:subPropertyOf viprs:n_lnoc_or_roc.

viso:can_run_on a owl:ObjectProperty;
  rdfs:comment "Relates a software tool to operating systems."^^xs:string;
  rdfs:domain viso:software_tool;
  rdfs:range viso:operating_system;
  rdfs:subPropertyOf viso:depends_on;
  rdfs:subPropertyOf viprs:n_lnoc_cor_roc.

viso:has_not_feature a owl:ObjectProperty;
  rdfs:domain viso:software_tool;
  rdfs:range viso:software_tool_feature;
  rdfs:comment "Note: this property should not be used as an alternative to a proper negation of has_feature. It was introduced to allow the reasoner to make deductions about non-features from the software_update entities. Also, it is supposed to be incompatible with (owl:propertyDisjointWith) has_feature, but the OWL language does not allow to impose this constraint"^^xs:string;
  rdfs:subPropertyOf viprs:immanent_relation.

viso:has_added_feature a owl:ObjectProperty;
  rdfs:domain viso:software_tool;
  rdfs:range viso:software_tool_feature;
  rdfs:subPropertyOf viso:has_feature;
  owl:propertyChainAxiom  (viso:is_output_of viso:adds_feature).

viso:has_acted_upon_feature a owl:ObjectProperty;
  rdfs:domain viso:software_tool;
  rdfs:range viso:software_tool_feature;
  rdfs:subPropertyOf viso:has_feature;
  owl:propertyChainAxiom  (viso:is_input_of viso:removes_feature).

viso:has_older_version a owl:ObjectProperty,
  owl:TransitiveProperty;
  rdfs:domain viso:software;
  rdfs:range viso:software;
  rdfs:comment "Note: this property is meant to be irreflexive (owl:IrreflexiveProperty), but the OWL language does not allow to impose this constraint"^^xs:string;
  owl:propertyChainAxiom  (viso:is_output_of viso:has_input);
  rdfs:subPropertyOf viso:has_other_version;
  rdfs:subPropertyOf viso:depends_on.

viso:has_newer_version a owl:ObjectProperty,
  owl:TransitiveProperty;
  rdfs:domain viso:software;
  rdfs:range viso:software;
  owl:propertyChainAxiom  (viso:is_input_of viso:has_output);
  owl:inverseOf viso:has_older_version;
  rdfs:subPropertyOf viso:has_other_version;
  rdfs:subPropertyOf viso:has_dependent.

viso:has_other_version a owl:ObjectProperty,
  owl:TransitiveProperty;
  a owl:SymmetricProperty;
  rdfs:domain viso:software;
  rdfs:range viso:software;
  rdfs:subPropertyOf viso:is_similar_to;
  rdfs:subPropertyOf viprs:shares_a_sign_with.

viso:has_lost_feature a owl:ObjectProperty;
  rdfs:domain viso:software_tool;
  rdfs:range viso:software_tool_feature;
  owl:propertyChainAxiom  (viso:is_output_of viso:removes_feature);
  rdfs:subPropertyOf viso:has_not_feature.  
  
viso:has_missing_feature a owl:ObjectProperty;
  rdfs:domain viso:software_tool;
  rdfs:range viso:software_tool_feature;
  owl:propertyChainAxiom  (viso:is_input_of viso:adds_feature);
  rdfs:subPropertyOf viso:has_not_feature.  

viso:is_modelling_twin_of a owl:ObjectProperty,
  owl:SymmetricProperty,
  owl:TransitiveProperty;
  rdfs:comment "Relates two objects that (despite being possibly distinct individuals) are equivalent from the modelling point of view. For example, a DPD thermostat is equivalent to the combination of Random and Drag forces, and a hard sphere can be represented using an infinite potential."^^xs:string;
  rdfs:subPropertyOf viso:is_similar_to;
  rdfs:subPropertyOf viprs:immanent_relation.

viso:involves a owl:ObjectProperty;
  rdfs:domain [a owl:Class; owl:unionOf (viso:software_tool_feature viso:modeling_related_entity)];
  rdfs:range [a owl:Class; owl:unionOf (vov:variable vov:function viso:model_object)];
  rdfs:comment "[X involves Y] means that there is a mathematical expression or an algorithmic formulation of X that contains the variable, function or model object Y"^^xs:string;
  rdfs:subPropertyOf viso:has_part.

viso:is_involved_in a owl:ObjectProperty;
  rdfs:domain [a owl:Class; owl:unionOf (vov:variable vov:function viso:model_object)];
  rdfs:range [a owl:Class; owl:unionOf (viso:software_tool_feature viso:modeling_related_entity)];
  owl:inverseOf viso:involves;
  rdfs:subPropertyOf viso:is_part_of.

viso:has_pe a owl:ObjectProperty;
  rdfs:domain viso:model_type;  
  rdfs:range viso:physical_equation;
  rdfs:subPropertyOf viso:has_part.

viso:is_pe_of a owl:ObjectProperty;
  rdfs:domain viso:physical_equation;  
  rdfs:range viso:model_type;
  owl:inverseOf viso:has_pe;
  rdfs:subPropertyOf viso:is_part_of.

viso:is_added_by_update a owl:ObjectProperty;
  rdfs:domain viso:software_tool_feature;
  rdfs:range viso:software_tool_update; 
  owl:inverseOf viso:adds_feature;
  rdfs:subPropertyOf viso:is_part_of.

viso:is_removed_by_update a owl:ObjectProperty;
  rdfs:domain viso:software_tool_feature;
  rdfs:range viso:software_tool_update; 
  owl:inverseOf viso:removes_feature;
  rdfs:subPropertyOf viso:is_part_of.

viso:has_input a owl:ObjectProperty;
  rdfs:domain viso:software_update;
  rdfs:range viso:software;
  rdfs:subPropertyOf viso:has_part.  # is_input_of [= is_part_of  =>  has_input [= has_part

viso:has_output a owl:ObjectProperty;
  rdfs:domain viso:software_update;
  rdfs:range viso:software;
  rdfs:subPropertyOf viso:has_part.  # is_output_of [= is_part_of  =>  has_output [= has_part

viso:adds_feature a owl:ObjectProperty;
  rdfs:domain viso:software_tool_update;
  rdfs:range viso:software_tool_feature;
  rdfs:subPropertyOf viso:has_part.

viso:removes_feature a owl:ObjectProperty;
  rdfs:domain viso:software_tool_update;
  rdfs:range viso:software_tool_feature;
  rdfs:subPropertyOf viso:has_part.

viso:has_interface a owl:ObjectProperty;
  rdfs:domain viso:software;
  rdfs:range viso:software_interface;
  rdfs:subPropertyOf viso:has_ability.

viso:has_classifier a owl:ObjectProperty;
  rdfs:domain [a owl:Class; owl:unionOf (viso:software_tool
  	      viso:software_tool_feature viso:modeling_related_entity)];
  rdfs:range viso:classifier;
  rdfs:subPropertyOf viso:has_documentation.

viso:has_clause a owl:ObjectProperty;
  rdfs:domain viso:license;
  rdfs:range viso:license_clause;
  rdfs:subPropertyOf swo:SWO_9001002.  # <-> SWO 'has clause'
  # rdfs:subPropertyOf viso:has_part.
swo:SWO_9001002 rdfs:subPropertyOf viso:has_part.

viso:is_mathematically_related_to a owl:ObjectProperty;
  rdfs:comment """[X is_mathematically_related_to Y] if X and Y are related to each other by a mathematical transformation, 
   	        such as, for example, differentiation, integration or Fourier transform."""^^xs:string;
  rdfs:domain evmpo:marketplace_related_entity;
  rdfs:range evmpo:marketplace_related_entity;
  a owl:SymmetricProperty;
  rdfs:subPropertyOf viso:is_connected_to;
  rdfs:subPropertyOf viprs:immanent_relation.

