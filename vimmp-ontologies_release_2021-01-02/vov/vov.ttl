######################   Object Properties      ########################################
    
vov:involves_object a owl:ObjectProperty; 
  rdfs:domain [a owl:Class; owl:unionOf (viso:software_tool_feature viso:modeling_related_entity)];
  rdfs:range viso:model_object;
  rdfs:subPropertyOf viso:involves.

vov:involves_variable a owl:ObjectProperty;
  rdfs:domain [a owl:Class; owl:unionOf (viso:software_tool_feature viso:modeling_related_entity)];
  rdfs:range vov:variable;
  rdfs:comment "[X involves_variable Y] means that there is a mathematical expression or an algorithmic formulation of X that contains the variable Y"^^xs:string;
  rdfs:subPropertyOf viso:involves.

vov:involves_function a owl:ObjectProperty;
  rdfs:domain [a owl:Class; owl:unionOf (viso:software_tool_feature viso:modeling_related_entity)];
  rdfs:range vov:function;
  rdfs:comment "[X involves_function Y] means that there is a mathematical expression or an algorithmic formulation of X that contains the function Y"^^xs:string;
  rdfs:subPropertyOf viso:involves.

vov:is_object_involved_in a owl:ObjectProperty;
  owl:inverseOf vov:involves_object;
  rdfs:subPropertyOf viso:is_involved_in;
  rdfs:domain viso:model_object;
  rdfs:range [a owl:Class; owl:unionOf (viso:software_tool_feature viso:modeling_related_entity)].
 
vov:is_variable_involved_in a owl:ObjectProperty;
   owl:inverseOf vov:involves_variable;
   rdfs:subPropertyOf viso:is_involved_in;
   rdfs:domain vov:variable;
   rdfs:range [a owl:Class; owl:unionOf (viso:software_tool_feature viso:modeling_related_entity)].

vov:is_function_involved_in a owl:ObjectProperty;
  owl:inverseOf vov:involves_function;
  rdfs:subPropertyOf viso:is_involved_in;
  rdfs:domain vov:function;
  rdfs:range [a owl:Class; owl:unionOf (viso:software_tool_feature viso:modeling_related_entity)].

vov:has_attached_variable a owl:ObjectProperty;
  rdfs:domain viso:model_object;
  rdfs:range vov:object_variable;
  rdfs:comment "[X has_attached_variable Y] means that the model object X (say, a particle) has an attached variable Y (say, its mass or velocity)"^^xs:string;
  rdfs:subPropertyOf vov:involves_variable.

vov:is_variable_attached_to a owl:ObjectProperty;
  rdfs:domain vov:object_variable;
  rdfs:range viso:model_object;
  owl:inverseOf vov:has_attached_variable;
  rdfs:subPropertyOf vov:is_variable_involved_in.

vov:has_mass a owl:ObjectProperty;
  a owl:FunctionalProperty;
  rdfs:subPropertyOf vov:has_attached_variable;
  rdfs:domain [a owl:Class; owl:unionOf (viso-am:model_object viso-el:model_object viso-co:model_object)];
  rdfs:range vov:object_mass.

vov:has_moment_of_inertia a owl:ObjectProperty;
  a owl:FunctionalProperty;
  rdfs:subPropertyOf vov:has_attached_variable;
  rdfs:domain viso-am:model_object;
  rdfs:range vov:object_moment_of_inertia.

vov:has_electric_charge a owl:ObjectProperty;
  a owl:FunctionalProperty;
  rdfs:subPropertyOf vov:has_attached_variable;
  rdfs:domain [a owl:Class; owl:unionOf (viso-am:model_object viso-el:model_object)];
  rdfs:range vov:object_electric_charge.

vov:has_electric_dipole_moment a owl:ObjectProperty;
  a owl:FunctionalProperty;
  rdfs:subPropertyOf vov:has_attached_variable;
  rdfs:domain viso-am:model_object;
  rdfs:range vov:object_electric_dipole_moment.

vov:has_electric_quadrupole_moment a owl:ObjectProperty;
  a owl:FunctionalProperty;
  rdfs:subPropertyOf vov:has_attached_variable;
  rdfs:domain viso-am:model_object;
  rdfs:range vov:object_electric_quadrupole_moment.

vov:has_position a owl:ObjectProperty;
  rdfs:subPropertyOf vov:has_attached_variable;
  rdfs:domain [a owl:Class; owl:unionOf (viso-am:model_object viso-el:classical_object viso-co:structureless_object)];
  rdfs:comment "Points to the position of a point in space."^^xs:string;
  rdfs:range vov:object_position.

vov:has_velocity a owl:ObjectProperty;
  rdfs:subPropertyOf vov:has_attached_variable;
  rdfs:domain [a owl:Class; owl:unionOf (viso-am:model_object viso-co:structureless_object)];
  rdfs:range vov:object_velocity.

vov:has_force a owl:ObjectProperty;
  rdfs:subPropertyOf vov:has_attached_variable;
  rdfs:domain viso-am:model_object;
  rdfs:range vov:object_force.

vov:has_attached_function a owl:ObjectProperty;
  rdfs:domain viso:model_object;
  rdfs:range vov:function;
  rdfs:comment "[X has_attached_function Y] means that the model object X (say, a particle or a fluid) has an attached function Y (say, a wavefunction or a velocity field)"^^xs:string;
  rdfs:subPropertyOf vov:involves_function.

vov:is_function_attached_to a owl:ObjectProperty;
  rdfs:domain vov:function;
  rdfs:range viso:model_object;
  owl:inverseOf vov:has_attached_function;
  rdfs:subPropertyOf vov:is_function_involved_in.

vov:has_wavefunction a owl:ObjectProperty;
  rdfs:domain viso-el:quantum_object;
  rdfs:range vov:wavefunction;
  rdfs:subPropertyOf vov:has_attached_function.

vov:has_velocity_field a owl:ObjectProperty;
  rdfs:domain viso:model_object;
  rdfs:range vov:velocity_field;
  rdfs:subPropertyOf vov:has_attached_function.

vov:has_initial_point a owl:ObjectProperty;
   rdfs:domain [a owl:Class; owl:unionOf (vov:relative_position vov:relative_velocity)];
   rdfs:range viso:model_object;
   rdfs:subPropertyOf viso:has_part.

vov:has_final_point a owl:ObjectProperty;
   rdfs:domain [a owl:Class; owl:unionOf (vov:relative_position vov:relative_velocity)];
   rdfs:range viso:model_object;
   rdfs:subPropertyOf viso:has_part.

vov:has_modulus a owl:ObjectProperty; 
  rdfs:domain vov:vector_variable;
  rdfs:range vov:scalar_variable;
  rdfs:subPropertyOf viso:has_part.

vov:has_direction a owl:ObjectProperty;
  rdfs:domain vov:vector_variable;
  rdfs:range vov:versor;
  rdfs:subPropertyOf viso:has_part.

vov:shares_value_with a owl:ObjectProperty;
  rdfs:domain vov:variable;
  rdfs:range vov:variable;
  a owl:SymmetricProperty;
  a owl:TransitiveProperty;    
  rdfs:subPropertyOf viprs:mereosemiotics_is_hs;
  rdfs:subPropertyOf viso:is_similar_to;
  owl:propertyChainAxiom (osmo:has_value [owl:inverseOf osmo:has_value]);
  rdfs:comment "[X shares_value_with Y] means that the variables X and Y have the same numerical value"^^xs:string.

vov:shares_role_with a owl:ObjectProperty;
  rdfs:domain vov:variable;
  rdfs:range vov:variable;
  a owl:SymmetricProperty;
  a owl:TransitiveProperty;
  rdfs:subPropertyOf viprs:immanent_relation;
  rdfs:subPropertyOf viso:is_similar_to;
  rdfs:comment """[X shares_role_with Y] means that the variables X and Y have the same role in the model or solver. 
This property can be used to instantiate variables that are already defined in VOV as individuals and to connect them to those.
For example, it can be used to relate the epsilon parameters of multiple Lennard-Jones potentials to each other and to the 
concept present in VOV."""^^xs:string.

