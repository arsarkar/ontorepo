vov:restriction a owl:Class;
   rdfs:subClassOf evmpo:marketplace_related_entity.

############################  (1) Variable  (Fundamental class) ########################

vov:variable a owl:Class;
  rdfs:comment "a variable in the mathematical sense, i.e. a symbol that stands for a quantity in a mathematical expression"^^xs:string;
  rdfs:subClassOf evmpo:marketplace_related_entity;
  rdfs:subClassOf osmo:logical_variable.

vov:object_variable a owl:Class;
  rdfs:subClassOf vov:variable;
  rdfs:subClassOf [a owl:Restriction;
  		  owl:onProperty vov:has_scope;
	    	  owl:hasValue "OBJECT"^^xs:string];
  rdfs:subClassOf [a owl:Restriction;
  		  owl:onProperty vov:is_variable_attached_to;
		  owl:qualifiedCardinality 1;
		  owl:onClass viso:model_object].  

vov:pair_variable a owl:Class;
  rdfs:subClassOf vov:variable;
  rdfs:subClassOf [a owl:Restriction;
  		  owl:onProperty vov:has_scope;
	    	  owl:hasValue "PAIR"^^xs:string];
  rdfs:subClassOf [a owl:Restriction;
  		  owl:onProperty vov:is_variable_involved_in;
		  owl:minQualifiedCardinality 2;
		  owl:onClass viso:model_object].  

vov:system_variable a owl:Class;
  rdfs:subClassOf vov:variable;
  rdfs:subClassOf [a owl:Restriction;
  		  owl:onProperty vov:has_scope;
	    	  owl:hasValue "SYSTEM"^^xs:string]. 

vov:universal_variable a owl:Class;
  rdfs:subClassOf vov:variable;
  rdfs:subClassOf [a owl:Restriction;
  		  owl:onProperty vov:has_scope;
	    	  owl:hasValue "UNIVERSE"^^xs:string]. 

vov:scalar_variable a owl:Class;
  rdfs:subClassOf vov:variable;
  rdfs:comment "A variable that is a scalar (with respect to real space)."^^xs:string;
  rdfs:comment "Whatever the spatial dimension of a system is, it is specified by just one element."^^xs:string.

vov:vector_variable a owl:Class;
  rdfs:comment "A variable that is a vector (with respect to real space)."^^xs:string;
  rdfs:comment "In a system of spatial dimension N, it is specified by N elements."^^xs:string;  
  rdfs:subClassOf vov:variable.

vov:tensor_variable a owl:Class;
  rdfs:comment "A variable that is a tensor (with respect to real space)."^^xs:string;
  rdfs:comment "In a system of spatial dimension N, it is specified by NxN elements."^^xs:string;  
  rdfs:subClassOf vov:variable.

vov:variable owl:disjointUnionOf (vov:object_variable vov:pair_variable vov:system_variable vov:universal_variable).
vov:variable owl:disjointUnionOf (vov:scalar_variable vov:vector_variable vov:tensor_variable).

vov:physical_variable a owl:Class;
  rdfs:subClassOf vov:variable;
  owl:equivalentClass [a owl:Restriction;
  		  owl:onProperty vov:is_physical;
	    	  owl:hasValue true];
  rdfs:subClassOf evmpo:property.

vov:model_variable a owl:Class;
  owl:intersectionOf (vov:variable 
    		     [a owl:Restriction;
  		     owl:onProperty viso:is_involved_in;
		     owl:someValuesFrom viso:model_feature]).

vov:mr_trait_variable a owl:Class;
  owl:intersectionOf (vov:variable 
    		     [a owl:Restriction;
  		     owl:onProperty viso:is_involved_in;
		     owl:someValuesFrom viso:materials_relation_trait]).

vov:pe_trait_variable a owl:Class;
  owl:intersectionOf (vov:variable 
    		     [a owl:Restriction;
  		     owl:onProperty viso:is_involved_in;
		     owl:someValuesFrom viso:physical_equation_trait]).

vov:ext_cond_trait_variable a owl:Class;
  owl:intersectionOf (vov:variable 
    		     [a owl:Restriction;
  		     owl:onProperty viso:is_involved_in;
		     owl:someValuesFrom viso:external_condition_trait]).

vov:solver_variable a owl:Class;
  owl:intersectionOf (vov:variable 
    		     [a owl:Restriction;
  		     owl:onProperty viso:is_involved_in;
		     owl:someValuesFrom viso:solver_feature]).

vov:relative_variable a owl:Class;
   rdfs:subClassOf vov:pair_variable.	

vov:relative_position a owl:Class;
   rdfs:subClassOf vov:position;
   rdfs:subClassOf vov:relative_variable;	
   rdfs:comment "The position of an object with respect to another one."^^xs:string;   
   rdfs:subClassOf [a owl:Restriction;
	       	   owl:onProperty vov:has_initial_point;
		   owl:qualifiedCardinality 1;
		   owl:onClass viso:model_object];
   rdfs:subClassOf [a owl:Restriction;
	       	   owl:onProperty vov:has_final_point;
		   owl:qualifiedCardinality 1;
		   owl:onClass viso:model_object].

vov:relative_velocity a owl:Class;
   rdfs:subClassOf vov:velocity;
   rdfs:subClassOf vov:relative_variable;	
   rdfs:comment "The velocity of an object with respect to another one."^^xs:string;   
   rdfs:subClassOf [a owl:Restriction;
	       	   owl:onProperty vov:has_initial_point;
		   owl:qualifiedCardinality 1;
		   owl:onClass viso:model_object];
   rdfs:subClassOf [a owl:Restriction;
	       	   owl:onProperty vov:has_final_point;
		   owl:qualifiedCardinality 1;
		   owl:onClass viso:model_object].

############################  (2) Function (Fundamental class)  ########################

vov:function a owl:Class;
  rdfs:comment "A relation between two or more variables (e.g., the radial distribution function, the energy density of states)."^^xs:string;
  rdfs:comment "It can be defined via a mathematical equation or via tabulated values."^^xs:string;  
  rdfs:subClassOf evmpo:marketplace_related_entity;
  rdfs:subClassOf [a owl:Restriction;
  		  owl:onProperty viso:involves;
		  owl:minQualifiedCardinality 2;
		  owl:onClass vov:variable].  

vov:pair_distribution_function a owl:Class;
  rdfs:subClassOf vov:function;
  rdfs:subClassOf [a owl:Restriction;
  		  owl:onProperty viso:involves;
		  owl:qualifiedCardinality 1;
		  owl:onClass vov:position].

vov:radial_distribution_function a owl:Class;
  rdfs:subClassOf vov:pair_distribution_function;
  rdfs:subClassOf [a owl:Restriction;
  		  owl:onProperty viso:involves;
		  owl:qualifiedCardinality 1;
		  owl:onClass vov:radial_distance];
  rdfs:subClassOf [a owl:Restriction;
  		  owl:onProperty viso:is_mathematically_related_to;
		  owl:qualifiedCardinality 1;
		  owl:onClass vov:isotropic_structure_factor].
  
vov:isotropic_structure_factor a owl:Class;
  rdfs:subClassOf vov:function;
  rdfs:subClassOf [a owl:Restriction;
  		  owl:onProperty viso:is_mathematically_related_to;
		  owl:qualifiedCardinality 1;
		  owl:onClass vov:radial_distribution_function];
  rdfs:subClassOf [a owl:Restriction;
  		  owl:onProperty viso:involves;
		  owl:qualifiedCardinality 1;
		  owl:onClass vov:linear_momentum].

vov:number_density_field a owl:Class;
   rdfs:subClassOf vov:scalar_field;
   rdfs:subClassOf [a owl:Restriction;
  		  owl:onProperty viso:involves;
		  owl:qualifiedCardinality 1;
		  owl:onClass vov:number_density]; 
   rdfs:subClassOf [a owl:Restriction;
  		  owl:onProperty viso:involves;
		  owl:qualifiedCardinality 1;
		  owl:onClass vov:position].

vov:trajectory a owl:Class;
   rdfs:subClassOf vov:function;
   rdfs:subClassOf [a owl:Restriction;
  		  owl:onProperty viso:involves;
		  owl:qualifiedCardinality 1;
		  owl:onClass vov:time]; 
   rdfs:subClassOf [a owl:Restriction;
  		  owl:onProperty viso:involves;
		  owl:qualifiedCardinality 1;
		  owl:onClass vov:position].

vov:autocorrelation_function a owl:Class;
   rdfs:subClassOf vov:function;
   rdfs:subClassOf [a owl:Restriction;
  		  owl:onProperty viso:involves;
		  owl:qualifiedCardinality 1;
		  owl:onClass vov:time].

vov:density_of_states a owl:Class;
   rdfs:subClassOf vov:function;
   rdfs:subClassOf [a owl:Restriction;
  		  owl:onProperty viso:involves;
		  owl:qualifiedCardinality 1;
		  owl:onClass vov:energy].

vov:field a owl:Class; 
  rdfs:subClassOf vov:function;
  rdfs:comment """A field is a function defining the values taken by a variable across space (i.e., as 
  	         a function of position)."""^^xs:string;
  rdfs:comment """Fields are mostly used in continuum descriptions, but also in others 
  (e.g., in the post-processing of atomistic-mesoscopic descriptions)."""^^xs:string;
  rdfs:subClassOf [a owl:Restriction;
  		  owl:onProperty viso:involves;
		  owl:qualifiedCardinality 1;
		  owl:onClass vov:position];
  owl:disjointUnionOf (vov:scalar_field vov:vector_field vov:tensor_field).

vov:scalar_field a owl:Class;
  rdfs:subClassOf vov:field.

vov:vector_field a owl:Class;
  rdfs:subClassOf vov:field.

vov:tensor_field a owl:Class;
  rdfs:subClassOf vov:field.

######     Functions that can only appear in electronic models     #####################

vov:electronic_density_of_states a owl:Class;
   rdfs:subClassOf vov:density_of_states;
   rdfs:subClassOf viso:R_IS_ELECTRONIC.

vov:electronic_density_field a owl:Class;
  rdfs:subClassOf vov:number_density_field;
  rdfs:subClassOf viso:R_IS_ELECTRONIC.

vov:wavefunction a owl:Class;
  rdfs:subClassOf vov:function; 
  rdfs:subClassOf viso:R_IS_ELECTRONIC.

vov:electronic_wavefunction a owl:Class;
  rdfs:subClassOf vov:wavefunction; 
  rdfs:subClassOf viso:R_IS_ELECTRONIC.

######     Functions (fields) that appear in continuum models      #####################

vov:temperature_field a owl:Class;
  rdfs:subClassOf vov:scalar_field;
  rdfs:subClassOf viso:R_IS_CONTINUUM;
  rdfs:subClassOf [a owl:Restriction;
  		  owl:onProperty viso:involves;
		  owl:qualifiedCardinality 1;
		  owl:onClass vov:temperature].

vov:pressure_field a owl:Class;
  rdfs:comment "Hydrostatic pressure field."^^xs:string;
  rdfs:subClassOf vov:scalar_field; 
  rdfs:subClassOf viso:R_IS_CONTINUUM;
  rdfs:subClassOf [a owl:Restriction;
  		  owl:onProperty viso:involves;
		  owl:qualifiedCardinality 1;
		  owl:onClass vov:pressure].

vov:deviatoric_stress_tensor_field a owl:Class;
  rdfs:subClassOf vov:tensor_field;
  rdfs:subClassOf viso:R_IS_CONTINUUM;
  rdfs:subClassOf [a owl:Restriction;
  		  owl:onProperty viso:involves;
		  owl:qualifiedCardinality 1;
		  owl:onClass vov:R_IS_PRESSURE].

vov:stress_tensor_field a owl:Class;
  rdfs:subClassOf vov:tensor_field; 
  rdfs:subClassOf viso:R_IS_CONTINUUM;
  rdfs:subClassOf [a owl:Restriction;
  		  owl:onProperty viso:involves;
		  owl:qualifiedCardinality 1;
		  owl:onClass vov:R_IS_PRESSURE].


vov:mass_density_field a owl:Class;
  rdfs:subClassOf vov:scalar_field;
  rdfs:subClassOf viso:R_IS_CONTINUUM;
  rdfs:subClassOf [a owl:Restriction;
  		  owl:onProperty viso:involves;
		  owl:qualifiedCardinality 1;
		  owl:onClass vov:mass_density].
		  
vov:velocity_field a owl:Class;
  rdfs:subClassOf vov:vector_field;
  rdfs:subClassOf viso:R_IS_CONTINUUM;
  rdfs:subClassOf [a owl:Restriction;
  		  owl:onProperty viso:involves;
		  owl:qualifiedCardinality 1;
		  owl:onClass vov:velocity].

vov:vorticity_field a owl:Class;
  rdfs:subClassOf vov:vector_field;
  rdfs:subClassOf viso:R_IS_CONTINUUM.

vov:scalar_order_parameter_field a owl:Class;
  rdfs:subClassOf vov:scalar_field;
  rdfs:subClassOf viso:R_IS_CONTINUUM.

