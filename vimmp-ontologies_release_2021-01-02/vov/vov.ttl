########################################################################################
#            EXAMPLE of STATEMENTS CONNECTING VISO-AM with VOV                         #
########################################################################################

# Any integrator will involve: vov:NUMBER_OF_TIME_STEPS, vov:TIME_STEP.

viso-am:integrator rdfs:subClassOf [a owl:Restriction;
      owl:onProperty viso:involves;
      owl:hasValue vov:NUMBER_OF_TIME_STEPS];
      rdfs:subClassOf [a owl:Restriction;
      owl:onProperty viso:involves;
      owl:hasValue vov:TIME_STEP];
      rdfs:subClassOf [a owl:Restriction;
      owl:onProperty viso:involves;
      owl:hasValue vov:NUMBER_OF_EQUILIBRATION_TIME_STEPS].

viso-am:barostat rdfs:subClassOf [a owl:Restriction;
      owl:onProperty viso:involves;
      owl:hasValue vov:TARGET_PRESSURE].

viso-am:thermostat rdfs:subClassOf [a owl:Restriction;
      owl:onProperty viso:involves;
      owl:hasValue vov:TARGET_TEMPERATURE].

# Any electrostatic_solver will involve (exactly) one variable from the class
# electrostatic_force_constant.

viso-am:electrostatics_solver rdfs:subClassOf [a owl:Restriction;
      owl:onProperty viso:involves;
      owl:qualifiedCardinality 1;
      owl:onClass vov:electrostatic_force_constant]. 

viso-am:electrostatic_potential rdfs:subClassOf [a owl:Restriction;
      owl:onProperty viso:involves;
      owl:qualifiedCardinality 1;
      owl:onClass vov:electrostatic_force_constant]. 

viso-am:COULOMB_POTENTIAL a [a owl:Restriction;
      owl:onProperty viso:involves;
      owl:qualifiedCardinality 2;
      owl:onClass vov:object_electric_charge]. 

viso-am:CHARGE_DIPOLE_POTENTIAL a [a owl:Restriction;
      owl:onProperty viso:involves;
      owl:qualifiedCardinality 1;
      owl:onClass vov:object_electric_charge];
      a [a owl:Restriction;
      owl:onProperty viso:involves;
      owl:qualifiedCardinality 1;
      owl:onClass vov:object_electric_dipole_moment].

viso-am:DIPOLE_DIPOLE_POTENTIAL a [a owl:Restriction;
      owl:onProperty viso:involves;
      owl:qualifiedCardinality 2;
      owl:onClass vov:object_electric_dipole_moment].

viso-am:CHARGE_QUADRUPOLE_POTENTIAL a [a owl:Restriction;
      owl:onProperty viso:involves;
      owl:qualifiedCardinality 1;
      owl:onClass vov:object_electric_charge];
      a [a owl:Restriction;
      owl:onProperty viso:involves;
      owl:qualifiedCardinality 1;
      owl:onClass vov:object_electric_quadrupole_moment].

viso-am:DIPOLE_QUADRUPOLE_POTENTIAL a [a owl:Restriction;
      owl:onProperty viso:involves;
      owl:qualifiedCardinality 1;
      owl:onClass vov:object_electric_dipole_moment];
      a [a owl:Restriction;
      owl:onProperty viso:involves;
      owl:qualifiedCardinality 1;
      owl:onClass vov:object_electric_quadrupole_moment].

viso-am:QUADRUPOLE_QUADRUPOLE_POTENTIAL a [a owl:Restriction;
      owl:onProperty viso:involves;
      owl:qualifiedCardinality 2;
      owl:onClass vov:object_electric_quadrupole_moment].

viso-am:charge_smearing rdfs:subClassOf [a owl:Restriction;
      owl:onProperty viso:involves;
      owl:hasValue vov:CHARGE_SMEARING_LENGTH].

viso-am:NVT viso:involves vov:TARGET_TEMPERATURE.

viso-am:NPT viso:involves vov:TARGET_TEMPERATURE,
  vov:TARGET_PRESSURE.

vov:DPD_THERMOSTAT_CUTOFF a vov:pair_variable;
  a vov:R_IS_LENGTH.

vov:DPD_THERMOSTAT_DRAG_COEFFICIENT a vov:pair_variable.

vov:GROOT_WARREN_REPULSION a vov:pair_variable.

vov:GROOT_WARREN_CUTOFF a vov:pair_variable;
  a vov:R_IS_LENGTH.
  
viso-am:DPD_THERMOSTAT viso:involves vov:DPD_THERMOSTAT_CUTOFF,
  vov:DPD_THERMOSTAT_DRAG_COEFFICIENT.

viso-am:GROOT_WARREN viso:involves vov:GROOT_WARREN_REPULSION,
  vov:GROOT_WARREN_CUTOFF.

viso-am:LENNARD_JONES_12_6 viso:involves vov:LJ_12_6_SIGMA,
  vov:LJ_12_6_EPSILON. 

vov:LJ_12_6_SIGMA a vov:pair_variable;
  a vov:length.

vov:LJ_12_6_EPSILON a vov:pair_variable;
  a vov:energy.

viso-am:LENNARD_JONES_12_6 a viso-am:mie_potential.

viso-am:mie_potential rdfs:subClassOf [a owl:Restriction;
      owl:onProperty viso:involves;
      owl:hasValue vov:MIE_REPULSIVE_EXPONENT];
      rdfs:subClassOf [a owl:Restriction;
      owl:onProperty viso:involves;
      owl:hasValue vov:MIE_ATTRACTIVE_EXPONENT];
      rdfs:subClassOf [a owl:Restriction;
      owl:onProperty viso:involves;
      owl:hasValue vov:MIE_SIGMA];
      rdfs:subClassOf [a owl:Restriction;
      owl:onProperty viso:involves;
      owl:hasValue vov:MIE_EPSILON].
      
vov:MIE_REPULSIVE_EXPONENT a vov:pair_variable;
  a vov:R_IS_DIMENSIONLESS.

vov:MIE_ATTRACTIVE_EXPONENT a vov:pair_variable;
  a vov:R_IS_DIMENSIONLESS.

vov:MIE_SIGMA a vov:pair_variable;
  a vov:length.

vov:MIE_EPSILON a vov:pair_variable;
  a vov:energy.

viso-am:mass_site rdfs:subClassOf [a owl:Restriction;
      owl:onProperty vov:has_attached_variable;
